{
	"name": "airdrop-svelte",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"build:server": "tsup server/index.ts",
		"start:server": "node dist/index.cjs",
		"start": "node build/index.js",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.85",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@types/express": "^4.17.13",
		"@types/ws": "^8.5.3",
		"@vitejs/plugin-basic-ssl": "^0.1.1",
		"autoprefixer": "^10.4.7",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.6",
		"tslib": "^2.3.1",
		"tsup": "^6.1.3",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lukeed/uuid": "^2.0.0",
		"body-parser": "^1.20.0",
		"cookie": "^0.5.0",
		"cors": "^2.8.5",
		"eventemitter3": "^4.0.7",
		"express": "^4.18.1",
		"heroicons": "github:skoshx/heroicons",
		"peerjs-js-binarypack": "^1.0.1",
		"tailwind-merge": "^1.5.0",
		"ua-parser-js": "^1.0.2",
		"webrtc-adapter": "^8.1.1",
		"ws": "^8.8.1"
	}
}
